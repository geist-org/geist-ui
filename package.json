{
  "name": "@geist-ui/core",
  "version": "2.3.8",
  "main": "dist/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "unpkg": "dist/index.min.js",
  "scripts": {
    "dev": "pnpm docs:collect && next dev",
    "docs:start": "next start",
    "docs:collect": "node scripts/collect-meta.js",
    "docs:seeds": "node scripts/seeds/update.js",
    "docs:build": "pnpm docs:collect & pnpm docs:seeds && next build",
    "prettier": "prettier --write .",
    "lint": "eslint \"{components,lib}/**/*.{js,ts,tsx}\"",
    "test": "jest --config .jest.config.js --no-cache",
    "test:update": "jest --config .jest.config.js --no-cache --update-snapshot",
    "test:coverage": "pnpm test --coverage",
    "build:clear": "rm -rf ./dist ./esm",
    "build:rollup": "rollup -c scripts/rollup.config.js",
    "build:webpack": "webpack --config scripts/webpack.config.js",
    "build:babel": "babel --no-babelrc --config-file ./scripts/babel.config.js --extensions \".js,.ts,.tsx\" ./components --out-dir ./esm --ignore \"**/__tests__/**/*,**/*.d.ts\"",
    "build:after": "node scripts/move-built-in.js",
    "build:types": "ttsc -p ./scripts & ttsc -p ./scripts --outDir ./esm",
    "build": "pnpm build:clear && pnpm build:rollup && pnpm build:babel && pnpm build:types && pnpm build:after",
    "release": "pnpm build && pnpm publish --access public --non-interactive"
  },
  "license": "MIT",
  "description": "Modern and minimalist React UI library.",
  "homepage": "https://geist-ui.dev",
  "bugs": {
    "url": "https://github.com/geist-org/geist-ui/issues/new/choose"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/geist-org/geist-ui"
  },
  "prettier": "@geist-ui/prettier-config",
  "keywords": [
    "geist",
    "geist ui",
    "components",
    "react components",
    "react ui"
  ],
  "files": [
    "/dist",
    "/esm"
  ],
  "sideEffects": false,
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/plugin-transform-runtime": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.2",
    "@geist-ui/icons": "^1.0.2",
    "@geist-ui/prettier-config": "^1.0.1",
    "@jest/expect": "^29.7.0",
    "@jest/globals": "^29.7.0",
    "@mapbox/rehype-prism": "^0.6.0",
    "@mdx-js/loader": "^1.6.22",
    "@mdx-js/react": "^3.0.0",
    "@next/bundle-analyzer": "^12.3.4",
    "@next/mdx": "^12.3.4",
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@testing-library/dom": "^8.20.1",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/enzyme": "^3.10.15",
    "@types/jest": "^26.0.24",
    "@types/mdx": "^2.0.9",
    "@types/react": "^18.2.33",
    "@types/react-color": "^3.0.9",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
    "babel-jest": "^27.5.1",
    "babel-plugin-transform-rename-import": "^2.3.0",
    "css-mediaquery": "^0.1.2",
    "enzyme": "^3.11.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-ts-lambdas": "^1.2.3",
    "eslint-plugin-react": "^7.33.2",
    "extract-mdx-metadata": "^1.0.0",
    "fs-extra": "^10.1.0",
    "jest": "^27.5.1",
    "next": "^14.0.1",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-live": "^2.4.1",
    "rehype-join-line": "^1.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-local-resolve": "^1.0.7",
    "styled-jsx": "4.0.1",
    "ts-jest": "^27.1.5",
    "ttypescript": "^1.5.15",
    "typescript": "^4.9.5",
    "typescript-transform-paths": "^3.4.6"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.2"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  }
}